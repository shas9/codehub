ll arr[100005];
ll brr[100005];

map < ll, ll > mp;

bool solve(ll n)
{
    mp.clear();

    for(ll i = 1; i <= n; i++)
    {
        ll diff = brr[i] - arr[i];

//        cout << diff << endl;
//
//        for(auto it: mp)
//        {
//            cout << it.first << " ~~ " << it.second << endl;
//        }

        cout << endl;
        if(diff < 0)
            if(mp.find(-1) == mp.end()) return 0;

        if(diff > 0)
            if(mp.find(1) == mp.end()) return 0;

        mp[arr[i]] = 1;
    }

    return 1;
}

